{
  "name": "ws-node-demo",
  "version": "1.0.0",
  "description": "An implmentation of tosa with Express, node 7.7.+, mongo, and JWT",
  "author": "Will Streeter <willstreeter@gmail.com> (http://www.willstreeter.com)",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/WillStreeter/ws-node-demo"
  },
  "scripts": {
    "check": "npm-check --skip-unused",
    "preinstall": "npm i -g nodemon node-inspector rimraf npm-run-all npm-check",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node ./dist/server.js",
    "start:dev": " npm-run-all -s clean build -p watch:*",
    "start:debug": "npm-run-all -s clean build -p watch:* debug",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "debug": "node-debug",
    "build": "npm-run-all -s build:swagger",
    "build:swagger": "npm run swagger-gen && npm run routes-gen && tsc && npm run copy-swagger-ui",
    "watch:tosa": "onchange './src/**/*.*' -d 3000 -- npm run swagger-gen && npm run routes-gen",
    "watch:node": "nodemon --inspect --delay 2.5 ./dist/server.js ",
    "watch:ts": "/usr/lib/node_modules/bin/tsc --watch",
    "lint": "tslint ./src/**/*.ts",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "tsoa routes",
    "copy-swagger-ui": "ncp ./src/presentation-layer/documentation/swagger-ui ./dist/swagger-ui",
    "tsoa": "tsoa"
  },
  "keywords": [
    "typescript",
    "swagger",
    "docker",
    "express"
  ],
  "engines": {
    "node": "~7.7.4"
  },
  "engineStrict": true,
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "class-validator":"^0.4.1 ",
    "cookie-parser": "^1.4.3",
    "config": "^1.21.0",
    "cors": "^2.7.1",
    "event-dispatch": "^0.4.1",
    "express": "^4.14.0",
    "express-ping": "^1.1.0",
    "express-winston": "^1.4.2",
    "method-override": "^2.3.6",
    "moment":"~2.18.0",
    "glob": "^7.0.5",
    "js-yaml": "^3.6.1",
    "jsonwebtoken": "~7.3.0",
    "mongodb": "^2.2.1",
    "mongoose": "^4.9.1",
    "reflect-metadata": "^0.1.3",
    "tsoa": "^1.1.8",
    "typedi": "^0.4.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "0.0.30",
    "@types/chai": "^3.4.35",
    "@types/config": "0.0.29",
    "@types/cookie-parser": "^1.3.30",
    "@types/cors": "0.0.29",
    "@types/express": "^4.0.30",
    "@types/js-yaml": "^3.5.25-alpha",
    "@types/jsonwebtoken": "~7.2.0",
    "@types/method-override": "0.0.27",
    "@types/mocha": "^2.2.40",
    "@types/mongodb": "^2.1.41",
    "@types/mongoose": "^4.7.1",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.18",
    "@types/serve-static": "^1.7.27",
    "@types/supertest": "^2.0.0",
    "@types/winston": "^2.2.0",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.3",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "ncp": "^2.0.0",
    "npm-run-all": "^3.0.0",
    "onchange": "^3.2.1",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.1",
    "supertest": "^1.2.0",
    "swagger-jsdoc": "^1.9.2",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.11.0",
    "typescript": "~2.2.0"
  }
}